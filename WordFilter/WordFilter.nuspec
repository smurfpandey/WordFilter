<?xml version="1.0"?>
<package >
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <title>$title$</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>https://github.com/smurfpandey/WordFilter/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/smurfpandey/WordFilter</projectUrl>    
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A small module meant for use in text generators that lets you filter strings for bad words. .net port of https://github.com/dariusk/wordfilter</description>
    <releaseNotes>First release.</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags>word filter blacklist</tags>
    <!-- Build actions for items in the contentFiles folder -->
    <contentFiles>
      <!-- Include Assets as Content -->
      <files include="any\any\badwords.json" buildAction="EmbeddedResource" />
    </contentFiles>
    <dependencies>
      <group>
        <dependency id="Newtonsoft.Json" version="11.0.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="badwords.json" target="contentFiles\any\any" />
  </files>
</package>